#%Module1.0#####################################################################
##
## modules tools/Reports-18.3
##
##
proc ModulesHelp { } {
        global version modroot

        puts stderr "tools/Reports-18.3 - sets the Environment for Arm Performance Reports 18.3 in my home directory"
}

module-whatis   "Sets the environment for using Performance Reports 18.3"

# for Tcl script use only
set     topdir          /opt/arm/Reports-18.3

prepend-path    PATH            $topdir/include
prepend-path    PATH            $topdir/bin
prepend-path    MANPATH         $topdir/man
prepend-path    LD_LIBRARY_PATH $topdir/lib
setenv		ALLINEA_LICENCE_DIR /opt/arm/licenses/

